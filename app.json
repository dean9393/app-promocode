{
  "expo": {
    "name": "promo-codes",
    "description": "A very interesting project.",
    "slug": "promo-codes",
    "privacy": "public",
    "sdkVersion": "29.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "ru.promocodehealth.promocodes",
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "YOUR_REASON_FOR_USING_USER_PHOTOS_HERE",
        "NSCameraUsageDescription": "YOUR_REASON_FOR_USING_USER_CAMERA_HERE",
        "NSPhotoLibraryAddUsageDescription": "would like to save photos to your photo gallery",
        "NSMicrophoneUsageDescription": "would like to your microphone (for videos)"
      }
    },
    "android": {
      "package": "ru.promocodehealth.promocodes",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
